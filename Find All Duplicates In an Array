class Solution:
    def findDuplicates(self, nums: List[int]) -> List[int]:
        from typing import List
        result = []       
        for num in nums:
            index = abs(num) - 1
            if nums[index] < 0:
                result.append(index + 1)
            else:
                nums[index] = -nums[index]
        for i in range(len(nums)):
            nums[i] = abs(nums[i])
        
        return result
